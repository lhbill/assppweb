{
  "name": "assppweb",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-23",
  "routes": [
    {
      "pattern": "asspp.lhbill.top",
      "custom_domain": true
    }
  ]
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./frontend/dist",
    "binding": "ASSETS",
  },
  "durable_objects": {
    "bindings": [
      { "name": "WISP_PROXY", "class_name": "WispProxy" },
      { "name": "DOWNLOAD_MANAGER", "class_name": "DownloadManager" },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["WispProxy", "DownloadManager"],
    },
  ],
  "kv_namespaces": [{ "binding": "AUTH_KV", "id": "6dc07ff7724f49c698dba5844cc6733d" }],
  "r2_buckets": [{ "binding": "IPA_BUCKET", "bucket_name": "assppweb-ipa","preview_bucket_name": "assppweb-ipa" }],
  "triggers": {
    "crons": ["0 2 * * *"],
  },
  "vars": {
    "AUTO_CLEANUP_DAYS": "2",
    "AUTO_CLEANUP_MAX_MB": "8192",
    // R2_CDN_DOMAIN: R2 public bucket custom domain for CDN-accelerated IPA downloads.
    // Set to your R2 custom domain (e.g. "cdn.example.com") after connecting it
    // in Dashboard → R2 → bucket → Settings → Public access → Custom Domains.
  },
}
